# Rclone Remote Names (should match names in rclone.conf)
OCI_RCLONE_REMOTE=oci-config
GCS_RCLONE_REMOTE=gcs-config

# OCI Tenancy OCID (your tenancy OCID)
OCI_TENANCY_OCID=

# GCS Configuration
GCS_STAGING_BUCKET=oci-focus-reports-staging
GCS_HIVE_BUCKET=oci-focus-reports-hive
GCS_PROJECT_ID=

# GCP Credentials (local only â€” on Cloud Run, the attached service account is used automatically)
GOOGLE_APPLICATION_CREDENTIALS=

# Job Configuration
SYNC_MODE=incremental
DAYS_TO_SYNC=7
DRY_RUN=false
LOG_LEVEL=info
